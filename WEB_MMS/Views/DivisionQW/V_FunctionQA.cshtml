        var SufeeAdmin = {

            cpuLoad: function () {

                var data = [],
                    totalPoints = 100;

                function getRandomData() {

                    if (data.length > 0)
                        data = data.slice(1);

                    // Do a random walk

                    while (data.length < totalPoints) {

                        //var prev = data.length > 0 ? data[data.length - 1] : 50,y = prev + Math.random() * 10 - 5;
